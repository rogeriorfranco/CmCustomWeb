<%@ Page Title="" Language="C#" MasterPageFile="~/Site.Master" AutoEventWireup="true" CodeBehind="change.aspx.cs" Inherits="CmCustomUi.facility.change" %>

<%@ Register Src="~/userControls/UserControlUpdateProgress.ascx" TagPrefix="uc1" TagName="UserControlUpdateProgress" %>


<asp:Content ID="Content1" ContentPlaceHolderID="ContentPlaceHolder" runat="server">
    <section class="content-header">
        <h1>Alteração Facilidades</h1>
    </section>
    <section class="content">
        <div class="box box-primary">
            <div class="box-body">
                <div class='row'>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Cto</label>
                            <asp:TextBox ID="txtCto" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Estação</label>
                            <asp:TextBox ID="txtEstacao" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-1">
                        <div class="form-group">
                            <label>Cnl</label>
                            <asp:TextBox ID="txtCnl" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Porta Cto</label>
                            <asp:TextBox ID="txtPortaCTO" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>Tecnologia</label>
                            <asp:TextBox ID="txtTecnologia" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Olt</label>
                            <asp:TextBox ID="txtOlt" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>


                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Modelo</label>
                            <asp:TextBox ID="txtModelo" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>IP Olt</label>
                            <asp:TextBox ID="txtIPOlt" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-1">
                        <div class="form-group">
                            <label>Shelf</label>
                            <asp:TextBox ID="txtShelf" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-1">
                        <div class="form-group">
                            <label>Slot</label>
                            <asp:TextBox ID="txtSlot" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-1">
                        <div class="form-group">
                            <label>Porta</label>
                            <asp:TextBox ID="txtPorta" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-1">
                        <div class="form-group">
                            <label>Ont ID</label>
                            <asp:TextBox ID="txtOnt" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Status</label>
                            <asp:DropDownList ID="ddlStatus" runat="server" class="form-control input-sm" ClientIDMode="Static">
                                <asp:ListItem>BLOQUEADO</asp:ListItem>
                                <asp:ListItem>DEFEITO</asp:ListItem>
                                <asp:ListItem>DESIGNADO</asp:ListItem>
                                <asp:ListItem>OCUPADO</asp:ListItem>
                                <asp:ListItem>VAGO</asp:ListItem>
                            </asp:DropDownList>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Circuito</label>
                            <asp:TextBox ID="txtCircuito" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>Vlan Inner</label>
                            <asp:TextBox ID="txtVlanInner" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>Vlan</label>
                            <asp:TextBox ID="txtVlan" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>Vlan Compartilhada</label>
                            <asp:TextBox ID="txtVlanCompartilhada" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Cliente</label>
                            <asp:TextBox ID="txtCliente" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Produto</label>
                            <asp:TextBox ID="txtProduto" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>Banda Mb</label>
                            <asp:TextBox ID="txtBandaMB" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>Banda Uplink</label>
                            <asp:TextBox ID="txtBandaUpLink" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        <div class="form-group">
                            <label>Serial</label>
                            <asp:TextBox ID="txtSerial" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Validade Reserva</label>
                            <asp:TextBox ID="txtDtValidadeReserva" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Pais</label>
                            <asp:TextBox ID="txtPais" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Localidade</label>
                            <asp:TextBox ID="txtLocalidade" runat="server" class="form-control input-sm" ClientIDMode="Static" ReadOnly="true"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Bairro</label>
                            <asp:TextBox ID="txtBairro" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Logradouro</label>
                            <asp:TextBox ID="txtLogradouro" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Número</label>
                            <asp:TextBox ID="txtNumeroLote" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Complemento 1</label>
                            <asp:TextBox ID="txtComplementoEndereco1" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Complemento Descrição 1</label>
                            <asp:TextBox ID="txtComplementoDescricaoEndereco1" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Complemento 2</label>
                            <asp:TextBox ID="txtComplementoEndereco2" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Complemento Descrição 2</label>
                            <asp:TextBox ID="txtComplementoDescricaoEndereco2" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Complemento 3</label>
                            <asp:TextBox ID="txtComplementoEndereco3" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Complemento Descrição 3</label>
                            <asp:TextBox ID="txtComplementoDescricaoEndereco3" runat="server" class="form-control input-sm" ClientIDMode="Static"></asp:TextBox>
                        </div>
                    </div>

                </div>
            </div>

            <div class="box-footer">
                <asp:LinkButton ID="lbSalvar" runat="server" CssClass="btn btn-primary" OnClick="lbSalvar_Click"><i class="fa fa-save"></i> Salvar</asp:LinkButton>
                <asp:LinkButton ID="lbLiberar" runat="server" CssClass="btn btn-primary"><i class="fa fa-undo"></i> Liberar</asp:LinkButton>
                <asp:LinkButton ID="lbLimpar" runat="server" CssClass="btn btn-primary"><i class="fa fa-eraser"></i> Limpar</asp:LinkButton>
            </div>

        </div>
    </section>
    <uc1:UserControlUpdateProgress runat="server" ID="UserControlUpdateProgress" />
</asp:Content>
