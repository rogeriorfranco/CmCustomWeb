$(document).ready(function () {
    $('#txtDtValidadeReserva').inputmask('99/99/9999');
});

$(document).ready(function () {
    $("#txtOnt").keypress(function (e) {
        if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
            return false;
        }
    });
});


